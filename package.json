{
  "name": "bluecarbon-ledger",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "esbuild index.tsx --bundle --outfile=index.js --format=esm --jsx=automatic --loader:.tsx=tsx --loader:.ts=ts --platform=browser --external:react --external:react-dom --external:react-dom/client --external:recharts --external:@google/genai --external:@supabase/supabase-js --define:process.env.API_KEY=\"$API_KEY\" --define:process.env.GOOGLE_MAPS_API_KEY=\"$GOOGLE_MAPS_API_KEY\" --define:process.env.SUPABASE_URL=\"$SUPABASE_URL\" --define:process.env.SUPABASE_ANON_KEY=\"$SUPABASE_ANON_KEY\"",
    "start": "npx serve -s . -l $PORT"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "@google/genai": "^1.34.0",
    "@supabase/supabase-js": "^2.39.3"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "serve": "^14.2.4",
    "typescript": "^5.7.3"
  }
}